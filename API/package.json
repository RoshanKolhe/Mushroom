{
  "name": "mushroom",
  "version": "0.0.1",
  "description": "mushroom",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": "16 || 18 || 20"
  },
  "scripts": {
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "lint": "npm run prettier:check",
    "lint:fix": "npm run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "pretest": "npm run rebuild",
    "test": "lb-mocha --allow-console-logs \"dist/__tests__\"",
    "posttest": "npm run lint",
    "test:dev": "lb-mocha --allow-console-logs dist/__tests__/**/*.js && npm run posttest",
    "docker:build": "docker build -t mushroom .",
    "docker:run": "docker run -p 3000:3000 -d mushroom",
    "premigrate": "npm run build",
    "migrate": "node ./dist/migrate",
    "preopenapi-spec": "npm run build",
    "openapi-spec": "node ./dist/openapi-spec",
    "prestart": "npm run rebuild",
    "start": "node -r source-map-support/register .",
    "clean": "lb-clean dist *.tsbuildinfo .eslintcache",
    "rebuild": "npm run clean && npm run build",
    "start:watch": "tsc-watch --target es2017 --outDir ./dist --onSuccess \"node .\"",
    "go": "npm run start:watch",
    "start:debug": "node --inspect ./dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "RoshanKolhe <kolheroshan508@gmail.com>",
  "license": "",
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@loopback/authentication": "^10.1.3",
    "@loopback/boot": "^6.1.4",
    "@loopback/core": "^5.1.4",
    "@loopback/cron": "^0.11.3",
    "@loopback/repository": "^6.1.5",
    "@loopback/rest": "^13.1.4",
    "@loopback/rest-explorer": "^6.1.4",
    "@loopback/service-proxy": "^6.1.4",
    "@types/multer": "^1.4.11",
    "axios": "^1.6.7",
    "bcryptjs": "^2.4.3",
    "crypto": "^1.0.1",
    "csv-parser": "^3.0.0",
    "dotenv": "^16.0.3",
    "firebase-admin": "^12.1.1",
    "isemail": "^3.2.0",
    "jsonwebtoken": "^8.5.1",
    "loopback-connector-mysql": "^5.4.4",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.0",
    "tslib": "^2.0.0",
    "twilio": "^4.21.0"
  },
  "devDependencies": {
    "@loopback/build": "^10.1.4",
    "@loopback/testlab": "^6.1.4",
    "@types/bcryptjs": "^2.4.2",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/lodash": "^4.14.195",
    "@types/node": "^16.18.68",
    "@types/nodemailer": "^6.4.7",
    "dotenv": "^16.0.3",
    "source-map-support": "^0.5.21",
    "tsc-watch": "^6.0.4",
    "typescript": "~5.2.2"
  }
}
